<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
        
        <t t-name="dynamic_snippet.s_carousel_EmployeeDetail">
                <t t-foreach="data" t-as="employee">
                        <ol>
                                <li>
                                        <t t-out="employee.name"></t>
                                        <!-- <t t-out="employee.image.decode('utf-8')"></t> -->
                                        <img src="{{ 'data:image/png;base64,%s' % employee.image.decode('utf-8') }}" style="height:100px; width:100px;"></img>
                                </li>
                        </ol>
                        <!-- <t t-out="employee.image"></t> -->
                </t>
        </t>
</templates>